{
    "name" : "Simpocity screen sharing",
    "author": "Simpocity",
    "version" : "3.4",
    "manifest_version" : 2,
    "minimum_chrome_version": "34",
    "description" : "Comparte tu pantalla completa o una aplicaci√≥n especifica",
    "homepage_url": "https://www.simpocity.com/",
    "background": {
        "scripts": ["background-script.js"],
        "persistent": false
    },
    "content_scripts": [ {
       "js": [ "content-script.js" ],
       "all_frames": true,
       "run_at": "document_end",
       "matches": ["https://pruebas.simpocity.com/*"]
    }],
    "icons" : {
        "48" : "icon.png"
    },
    "permissions": [
        "desktopCapture"
    ],
    "web_accessible_resources": [
        "icon.png"
    ]
}